<div class="carousel">
  <div class="items cycle-slideshow" 
    data-cycle-slides=">.item"
    data-cycle-prev=#prev
    data-cycle-next=#next
    data-cycle-auto-height=container>
    {% for item in items %}
      <div class="item {{ 'item-' ~ loop.index }}">
        {{ item }}
      </div>
    {% endfor %}
  </div>
  <div class="controls">
    <div id="prev">Anterior</div>
    <div id="next">Siguiente</div>
  </div>
</div>
{{ attach_library('tierras/carousel') }}
